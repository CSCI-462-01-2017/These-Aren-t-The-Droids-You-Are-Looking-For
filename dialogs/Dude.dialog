	local filename = user_input_string("Please enter the dialog file name of the desired NPC. Example: Chandra. If the name is invalid, the game will crash! To abort, enter 'exit' or press enter or escape.")
	if (not (filename == "exit" or filename == "")) then
		tux_says(_"Starting " .. filename)
		start_chat(filename)
	end
